<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mjcbrothers.dronetoyou.member.MemberMapper">
	<select id="getMemberByID" parameterType="com.mjcbrothers.dronetoyou.member.Member" resultType="com.mjcbrothers.dronetoyou.member.Member">
		select * from dronetou_member where dm_id = #{dm_id}
	</select>
	<insert id="join" parameterType="com.mjcbrothers.dronetoyou.member.Member">
		insert into dronetou_member values(#{dm_id}, #{dm_pw}, #{dm_name}, #{dm_phone}, #{dm_birthday}, #{dm_email}, #{dm_photo})
	</insert>
</mapper>